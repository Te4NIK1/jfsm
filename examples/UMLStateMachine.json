{
  "name": "UMLStateMachine",
  "model": "moore",
  "events": [{"name": "EventOne", "type": "input"}, 
  			 {"name": "EventTwo", "type": "output"}],
  "data": [{"name": "dataOne", "type": "int"}, {"name": "dataTwo", "type": "int"}],
  "states": [{"name": "StateOne", "onEntry":true, "onTick":true, "onExit":true}, 
             {"name": "StateTwo", "output": "EventTwo", "onEntry":true, "onTick":true, "onExit":true}],
  "transitions": [{"name": "TransitionOne", "from": "StateOne", "to": "StateTwo", "trigger": "EventOne", "onTransition":true},
                  {"name": "TransitionTwo", "from": "StateTwo", "to": "StateOne", "trigger": "true", "onTransition":true}],
  "initialState": "StateOne"
}