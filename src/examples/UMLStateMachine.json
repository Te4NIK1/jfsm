{
  "name": "UMLStateMachine",
  "model": "moore",
  "events": [
    {"name": "EventOne", "type": "input"}, 
  	{"name": "EventTwo", "type": "output"},
    {"name": "EventThree", "type": "output"}
  ],
  "data": [
    {"name": "dataOne", "type": "int"}, 
    {"name": "dataTwo", "type": "int"}
  ],
  "states": [
    {"name": "StateOne", "onEntry":true, "onTick":true, "onExit":true}, 
    {"name": "StateTwo", "onEntry":true, "onTick":true, "onExit":true},
    {"name": "StateThree", "output": "EventTwo", "onEntry":true, "onTick":true, "onExit":true}
  ],
  "transitions": [
    {"name": "TransitionOne", "from": "StateOne", "to": "StateTwo", "trigger": "EventOne", "onTransition":true},
    {"name": "TransitionTwo", "from": "StateTwo", "to": "StateTwo", "trigger": "EventOne", "guard": "dataOne > 5", "onTransition":true},
    {"name": "TransitionThree", "from": "StateTwo", "to": "StateOne", "trigger": "EventOne", "guard": "dataOne > 5", "onTransition":true}
  ],
  "initialState": "StateOne"
}